<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane fx:id="mainWindow" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.graphvisualizer.HelloController">
   <children>
      <TextField fx:id="vortexChooser" layoutX="14.0" layoutY="14.0" onAction="#handleTextEdit" promptText="enter id of vortex" text="0" />
      <Button fx:id="prev" layoutX="14.0" layoutY="39.0" mnemonicParsing="false" onMouseClicked="#handlePrevious" prefHeight="25.0" prefWidth="75.0" text="Previous" visible="false" />
      <Button fx:id="next" layoutX="88.0" layoutY="39.0" mnemonicParsing="false" onMouseClicked="#handleNext" prefHeight="25.0" prefWidth="75.0" text="Next" />
      <Button fx:id="clear" layoutX="14.0" layoutY="64.0" mnemonicParsing="false" onMouseClicked="#handleClear" prefHeight="25.0" prefWidth="149.0" text="Clear" />
   </children>
</Pane>
